{
  "name": "nestjs-project-structure",
  "version": "0.0.0",
  "description": "Node.js framework NestJS project structure",
  "main": "dist/app",
  "type": "module",
  "engines": {
    "node": ">=20.19.0 || >=22.12.0"
  },
  "scripts": {
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "entity:load": "ts-node bin/entity",
    "entity:sync": "typeorm-ts-node-commonjs -d bin/ormconfig.ts schema:sync",
    "build": "nest build",
    "prestart": "npm run lint && npm run build",
    "start": "node dist/app",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:repl": "nest start --entryFile repl",
    "test": "vitest run --exclude **/e2e/**/*.ts",
    "test:e2e": "vitest run --config ./test/vitest.e2e.ts",
    "doc": "compodoc -p tsconfig.json -s -d docs",
    "doc:api": "npm run build && node dist/swagger"
  },
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "@nestjs/apollo": "^13.0.3",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.11",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.11",
    "@nestjs/graphql": "^13.0.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.11",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.0.6",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "axios": "^1.8.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.0",
    "express-session": "^1.18.1",
    "graphql": "^16.10.0",
    "helmet": "^8.0.0",
    "mysql2": "^3.13.0",
    "nanoid": "^5.1.3",
    "nestjs-pino": "^4.3.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.21"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.26",
    "@nestjs/cli": "^11.0.5",
    "@nestjs/testing": "^11.0.11",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/node": "^22.13.10",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/prompts": "^2.4.9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/eslint-plugin": "1.1.34",
    "esbuild": "^0.25.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-sonarjs": "^2.0.4",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "prompts": "^2.4.2",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "typeorm-model-generator": "^0.4.6-no-engines",
    "typescript": "~5.8.2",
    "vitest": "^3.0.8",
    "vitest-mock-extended": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CatsMiaow/nestjs-project-structure.git"
  },
  "keywords": [
    "Node.js",
    "NestJS",
    "TypeScript"
  ],
  "homepage": "https://github.com/CatsMiaow/nestjs-project-structure#readme",
  "author": "CatsMiaow",
  "license": "MIT"
}
